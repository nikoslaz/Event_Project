<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>RegistrationClient</servlet-name>
        <servlet-class>servlets.RegistrationClient</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadEvents</servlet-name>
        <servlet-class>servlets.LoadEvents</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadClients</servlet-name>
        <servlet-class>servlets.LoadClients</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddEvent</servlet-name>
        <servlet-class>servlets.AddEvent</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateClientTable</servlet-name>
        <servlet-class>servlets.CreateClientTable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateTables</servlet-name>
        <servlet-class>servlets.CreateTables</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadTickets</servlet-name>
        <servlet-class>servlets.LoadTickets</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadReservations</servlet-name>
        <servlet-class>servlets.LoadReservations</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadClientEvents</servlet-name>
        <servlet-class>servlets.LoadClientEvents</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationClient</servlet-name>
        <url-pattern>/RegistrationClient</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadEvents</servlet-name>
        <url-pattern>/LoadEvents</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadClients</servlet-name>
        <url-pattern>/LoadClients</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddEvent</servlet-name>
        <url-pattern>/AddEvent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateClientTable</servlet-name>
        <url-pattern>/CreateClientTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateTables</servlet-name>
        <url-pattern>/CreateTables</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadTickets</servlet-name>
        <url-pattern>/LoadTickets</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadReservations</servlet-name>
        <url-pattern>/LoadReservations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadClientEvents</servlet-name>
        <url-pattern>/LoadClientEvents</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
